<?xml version="1.0" encoding="UTF-8" standalone="yes"?><userScript xmlns="http://www.approuter.com/schemas/2003/1/UserCallouts/"><functionName>validateDate</functionName><arguments><argument required="true" type="string" name="LastPostingDate"/></arguments><result type="string"/><body>if (LastPostingDate =='0000-00-00')
{
return null;
}
else
{
return LastPostingDate;
}</body></userScript>